{
    "name": "backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node ./bin/www",
        "dev": "nodemon ./bin/www",
        "createdb": "node_modules/.bin/sequelize db:create",
        "migrate": "node_modules/.bin/sequelize db:migrate",
        "makemigration": "node node_modules/sequelize-auto-migrations/bin/makemigration.js --name",
        "seed": "node_modules/.bin/sequelize db:seed:all",
        "pretty": "prettier --write \"./**/*.{js,jsx,json}\" ./bin/www",
	"prestart" : "bash prestart.sh"
    },
    "dependencies": {
        "acorn": ">=5.7.4",
        "clean-css": ">=4.1.11",
        "connect-redis": "^4.0.4",
        "constantinople": ">=3.1.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "easygraphql-format-error": "0.0.3",
        "express": "~4.16.1",
        "express-session": "^1.17.0",
        "express-graphql": "^0.9.0",
        "graphql": "^14.6.0",
        "graphql-tools": "^4.0.7",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "kind-of": ">=6.0.3",
        "morgan": "~1.9.1",
        "mysql": "^2.18.1",
        "mysql2": "^2.1.0",
        "passport": "^0.4.1",
        "passport-facebook": "^3.0.0",
        "redis": "^3.0.2",
        "redis-server": "^1.2.2",
        "sequelize": "^5.21.5",
        "sequelize-auto-migrations": "git+https://git@github.com/scimonster/sequelize-auto-migrations.git#a063aa6535a3f580623581bf866cef2d609531ba",
        "sequelize-cli": "^5.5.1",
        "util": "^0.12.2",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "husky": "^4.2.3",
        "nodemon": "^2.0.2",
        "prettier": "2.0.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run pretty"
        }
    }
}
